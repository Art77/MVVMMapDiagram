        <ResourceDictionary Source="Resources/DesignerItems/TrafficLightIItemDataTemplate.xaml">
        </ResourceDictionary>
      </ResourceDictionary.MergedDictionaries>
    </ResourceDictionary>
  </Application.Resources>
</Application>