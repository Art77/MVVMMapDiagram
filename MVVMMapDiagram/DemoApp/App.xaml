        </ResourceDictionary>
        <ResourceDictionary Source="pack://application:,,,/DiagramDesigner;component/Resources/Styles/ToolTip.xaml">
        </ResourceDictionary>
        <ResourceDictionary Source="pack://application:,,,/DiagramDesigner;component/Resources/Styles/DesignerItems.xaml">
        </ResourceDictionary>
        <ResourceDictionary Source="pack://application:,,,/DiagramDesigner;component/Resources/ConnectorItemStyle.xaml">
        </ResourceDictionary>
        <ResourceDictionary Source="pack://application:,,,/DiagramDesigner;component/Resources/DesignerStyle.xaml">
